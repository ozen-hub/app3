<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainFormController">
   <children>
      <TextField fx:id="txtUsername" layoutX="14.0" layoutY="14.0" onAction="#txtUsernameInputOnAction" onKeyPressed="#txtUserNameKeyPressOnAction" prefHeight="25.0" prefWidth="270.0" promptText="input username" />
      <Button layoutX="231.0" layoutY="49.0" mnemonicParsing="false" onAction="#btnPrintOnAction" text="print">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField fx:id="txtOutputUsername" layoutX="14.0" layoutY="100.0" prefHeight="25.0" prefWidth="270.0" promptText="fill with button press" />
      <TextField fx:id="txtOutputUsernameFillWithEnter" layoutX="14.0" layoutY="139.0" prefHeight="25.0" prefWidth="270.0" promptText="fill with enter" />
      <TextField fx:id="txtOutputUsernameWithKeyPress" layoutX="14.0" layoutY="175.0" prefHeight="25.0" prefWidth="270.0" promptText="fill with key press" />
   </children>
</AnchorPane>
